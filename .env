# 数据库连接配置
# 数据库配置 - 使用SQLite
DB_PATH=./database/star_shopping.db

# 会话密钥，用于加密和验证会话
SESSION_SECRET=your_secret_key
JWT_SECRET=your_jwt_secret

# 服务器运行端口
PORT=3000
HOST=0.0.0.0
WIFI_IFACE=
TLS_ENABLED=0
TLS_KEY_PATH=
TLS_CERT_PATH=

# 管理员用户配置
# 可以设置多个管理员用户ID，用逗号分隔
ADMIN_USER_IDS=admin,administrator,root
