    <style>
        .dashboard-card {
            border: none;
            border-radius: 15px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            transition: transform 0.2s;
        }

        .dashboard-card:hover {
            transform: translateY(-2px);
        }

        .metric-card {
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
        }

        .chart-container {
            position: relative;
            height: 400px;
            margin: 20px 0;
        }

        .recommendation-item {
            border-left: 4px solid #007bff;
            padding-left: 15px;
            margin-bottom: 15px;
        }

        .priority-high {
            border-left-color: #dc3545;
        }

        .priority-medium {
            border-left-color: #ffc107;
        }

        .priority-low {
            border-left-color: #28a745;
        }

        .sidebar {
            background: #f8f9fa;
            min-height: 100vh;
            padding: 20px 0;
        }

        .nav-link {
            color: #495057;
            border-radius: 8px;
            margin: 2px 0;
        }

        .nav-link:hover,
        .nav-link.active {
            background-color: #007bff;
            color: white;
        }
        .terminal-window {
            background:#0c0c0c;
            color:#0f0;
            font-family: 'Courier New', monospace;
            padding:12px;
            border-radius:10px;
            height:400px;
            overflow-y:auto;
            line-height:1.5;
        }
        .terminal-header{
            background:#121212;
            color:#9ff39f;
            font-family: 'Courier New', monospace;
            border-radius:10px 10px 0 0;
            padding:8px 12px;
            display:flex; align-items:center; gap:10px;
            border:1px solid #00aa55;
            border-bottom:none;
        }
        .terminal-dot{ width:10px; height:10px; border-radius:50%; display:inline-block; }
        .dot-red{ background:#ff5f56; }
        .dot-yellow{ background:#ffbd2e; }
        .dot-green{ background:#27c93f; }
        .terminal-title{ flex:1; text-align:center; color:#9ff39f; }
        .terminal-container{ border:1px solid #00aa55; border-top:none; border-radius:0 0 10px 10px; }
        .terminal-line { white-space: pre; }
        .log-line { white-space: pre; }
        .t-method { color:#0f0; }
        .t-path { color:#c9f7c9; }
        .t-time { color:#9ca3af; }
        .t-size { color:#9ca3af; }
        .t-status.ok { color:#10b981; }
        .t-status.redir { color:#22d3ee; }
        .t-status.warn { color:#f59e0b; }
        .t-status.err { color:#ef4444; }
        .terminal-cursor{ display:inline-block; width:8px; height:1em; background:#0f0; animation: blink 1s steps(1,end) infinite; vertical-align:baseline; }
        @keyframes blink { 50% { opacity:0; } }
        #server-monitor { background:#000; padding:12px; border-radius:12px; }
        #server-monitor, #server-monitor .card, #server-monitor .card-body, #server-monitor .card-header {
            background:#000; color:#e5ffe5; border-color:#00aa55;
        }
        #server-monitor .dashboard-card { border:1px solid #00aa55; box-shadow:none; }
        #server-monitor .card-header h5 { color:#e5ffe5; }
        #server-monitor small, #server-monitor .text-muted { color:#9ff39f !important; }
        .log-item-error { background:#ffe6e6; color:#cc0000; font-family:'Courier New', monospace; border-bottom:1px solid #f5b3b3; padding:8px 10px; }
    </style>
</head>

